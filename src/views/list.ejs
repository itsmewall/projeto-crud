<!DOCTYPE html>
<html lang="pt-br">
    <%- include('partials/head.ejs') %>
<body>
    <h1><%= title %></h1>

    <%- include('partials/navbar.ejs') %>


    <ul>
        <% users.forEach((user) => { %>
            <li>
                <%= user.name %> - <%= user.email %> - Idade: <%= user.age %>
                <a href="/edit?id=<%= user._id %>">[editar]</a>
            </li>
        <% }) %>
    </ul>

    <script src="js/main.js"></script>
    <%- include('partials/foot.ejs') %>
</body>

</html>